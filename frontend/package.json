{
  "name": "Mailer App",
  "version": "0.1.0",
  "private": true,
  "author": "Aundrey Drummond, Josh Friese",
  "scripts": {
    "build:dev": "node_modules/webpack/bin/webpack.js --env=dev",
    "build:prod": "node_modules/webpack/bin/webpack.js --env=prod -p",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build-dev": "concurrently \"npm run-script build:dev\" \"npm run-script test:watch\" ",
    "build-prod": "concurrently \"node_modules/webpack/bin/webpack.js --env=prod -p\" \"node_modules/jest/bin/jest.js\""
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "testResultsProcessor": "./node_modules/jest-junit"
  },
  "jest-junit": {
    "suiteName": "ROCC Unit Tests",
    "output": "./__test__/junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
"dependencies": {
    "axios": "^0.16.2",
    "bootstrap":"^3.3.7",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-icons": "^2.2.5",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "concurrently": "^3.5.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "html-webpack-plugin": "^2.29.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jest-css-modules": "^1.1.0",
    "jest-html-reporter": "^0.5.1",
    "jest-junit": "^2.1.0",
    "json-loader": "^0.5.4",
    "react-test-renderer": "^15.6.1",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-mock-store": "^1.2.3",
    "sass": "^1.0.0-beta.2",
    "sass-loader": "^6.0.6",
    "sinon": "^2.3.8",
    "style-loader": "^0.18.2",
    "uglify-js": "^3.0.25",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.3.0",
    "webpack-bundle-tracker": "^0.2.0"
  }

}
